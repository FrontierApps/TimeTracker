
<%= form_for :timerecord, url: timerecords_path do |f| %>
  <p>
    <%= params[:job_id]%>
  	
  </p>
  <p>
  <%= f.hidden_field :jobnumber %>
    
  </p>
  <p>
    <%= f.label :taskname %><br>
    <%= f.collection_select :task_id, Task.order(:taskname),:id, :taskname, include_blank: true %>
  </p>
    
 
  <p>
    <%= f.submit "Time In"%>	
  </p>
<% end %>